{
  "expo": {
    "name": "leaf-buddy",
    "slug": "leaf-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-color.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "500x500",
      "backgroundColor": "#7FC37E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-color.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jayantita.leafbuddy"
    },
    "web": {
      "favicon": "./assets/logo-color.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "39493342-55a0-451a-ace1-7778c6861ab6"
      }
    }
  }
}
